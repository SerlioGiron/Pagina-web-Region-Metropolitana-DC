{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/MTailwind.jsx", "src/components/Carousel.jsx", "src/app/Blog-page/page.jsx", "src/app/Blog-page/page.jsx", "src/components/Establecimientos.jsx", "src/components/Descargas.jsx", "src/components/Galeria.jsx", "src/components/Footer.jsx", "src/components/StickyNavbar.jsx", "src/components/Preguntas.jsx", "src/components/Noticias.jsx", "src/components/HorizontalCard.jsx"],
  "exclude": ["node_modules"]
}
